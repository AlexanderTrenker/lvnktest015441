   <scripts>
      <row>
         <pre_process><![CDATA[:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.PREPARE_RA_JOB
:SET &CONN_ENDPOINT_URL# = &UC4RB_BROCADE_URL#
:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.SET_CONNECTION_AUTH_BASIC

: SET &UC4RB_BROCADE_F_PORTS# = ""
: SET &UC4RB_REQUEST_BODY# ="<n-port-map><n-port>&UC4RB_BROCADE_N_PORT#</n-port><configured-f-port-list>"

: DEFINE &STRINGS#, string, 70
: FILL &STRINGS#[] = STR_SPLIT(&UC4RB_BROCADE_F_PORT#,",")
: SET &VAR# = 1
: SET &LEN# = LENGTH(&STRINGS#[])
:  WHILE &VAR# LE &LEN#
:    IF &STRINGS#[&VAR#] NE ""
:      SET &TEMP# = "<f-port>&STRINGS#[&VAR#]</f-port>"
:      SET &UC4RB_BROCADE_PORTS# = STR_CAT(&UC4RB_BROCADE_PORTS#,&TEMP#)
:     ENDIF
:   SET &VAR# = &VAR#+ 1
:  ENDWHILE

:   SET &UC4RB_REQUEST_BODY# = STR_CAT(&UC4RB_REQUEST_BODY#,&UC4RB_BROCADE_PORTS#)
:   SET &UC4RB_REQUEST_BODY# = STR_CAT(&UC4RB_REQUEST_BODY#," </configured-f-port-list></n-port-map>")

]]></pre_process>
      </row>
   </scripts>
