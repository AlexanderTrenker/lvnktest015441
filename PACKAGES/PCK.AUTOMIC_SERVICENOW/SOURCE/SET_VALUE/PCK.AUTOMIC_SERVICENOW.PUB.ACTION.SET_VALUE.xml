:SET &UC4RB_SN_TABLE# = &UC4RB_SN_SENITIZED_STRING#
:SET &UC4RB_SN_SENITIZED_STRING# = &UC4RB_SN_FIELD_NAME#
:INC PCK.AUTOMIC_SERVICENOW.PUB.INCLUDE.FIELDS_SENITATION
:SET &UC4RB_SN_FIELD# = &UC4RB_SN_SENITIZED_STRING#
:SET &UC4RB_SN_ESCAPE_STRING# = &UC4RB_SN_FIELD_VAL#
:INC PCK.AUTOMIC_SERVICENOW.PUB.INCLUDE.ESCAPE_STRING
:SET &UC4RB_SN_VALUE# = &UC4RB_SN_ESCAPE_STRING#
:INC PCK.AUTOMIC_SERVICENOW.PUB.INCLUDE.API_VERSION

]]></pre_process>
      </row>
      <row>
